<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fertisales">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfitGraph"></activity>
        <activity android:name=".SaleReportList" />
        <activity android:name=".SaleReport" />
        <activity android:name=".SupplierList" />
        <activity android:name=".ProductList" />
        <activity android:name=".CustomersList" />
        <activity android:name=".Sale_To_Farmer" />
        <activity android:name=".Add_Stock" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>